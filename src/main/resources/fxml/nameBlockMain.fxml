<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="90.0" prefWidth="1240.0" style="-fx-background-color: #ffffff;" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <right>
        <HBox spacing="25.0" BorderPane.alignment="CENTER">
            <children>
                <StackPane fx:id="playButton" onMouseClicked="#onPlayClicked">
                    <children>
                        <Circle fill="#1f93ff00" radius="30.0" stroke="#79bcee" strokeType="INSIDE" strokeWidth="3.0" />
                        <ImageView fitHeight="35.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/play.png" />
                            </image>
                            <StackPane.margin>
                                <Insets left="5.0" />
                            </StackPane.margin>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane>
                    <children>
                        <StackPane fx:id="unsetBadQualityButton" onMouseClicked="#unsetBadQualityClicked">
                            <children>
                                <Circle fill="#1f93ff00" radius="30.0" stroke="#ff9244" strokeType="INSIDE" strokeWidth="3.0" />
                                <ImageView fitHeight="35.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/thumbs-down.png" />
                                    </image>
                                    <StackPane.margin>
                                        <Insets />
                                    </StackPane.margin>
                                </ImageView>
                            </children>
                        </StackPane>
                        <StackPane fx:id="setBadQualityButton" onMouseClicked="#setBadQualityClicked">
                            <children>
                                <Circle fill="#1f93ff00" radius="30.0" stroke="#999999" strokeType="INSIDE" strokeWidth="3.0" />
                                <ImageView fitHeight="35.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/thumbs-down-off.png" />
                                    </image>
                                </ImageView>
                            </children>
                        </StackPane>
                    </children>
                </StackPane>
                <StackPane onMouseClicked="#removeButtonClicked">
                    <children>
                        <Circle fill="#1f93ff00" radius="30.0" stroke="#e47474" strokeType="INSIDE" strokeWidth="3.0" />
                        <ImageView fitHeight="35.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/delete.png" />
                            </image>
                            <StackPane.margin>
                                <Insets />
                            </StackPane.margin>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane>
                    <children>
                        <StackPane fx:id="expandoExpand" onMouseClicked="#onExpandClicked">
                            <children>
                                <Circle fill="#1f93ff00" radius="30.0" stroke="#7991eb" strokeType="INSIDE" strokeWidth="3.0" />
                                <ImageView fitHeight="50.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/more.png" />
                                    </image>
                                    <StackPane.margin>
                                        <Insets />
                                    </StackPane.margin>
                                </ImageView>
                            </children>
                        </StackPane>
                        <StackPane fx:id="expandoUnexpand" onMouseClicked="#onUnexpandClicked">
                            <children>
                                <Circle fill="#1f93ff00" radius="30.0" stroke="#666666" strokeType="INSIDE" strokeWidth="3.0" />
                                <ImageView fitHeight="50.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/less.png" />
                                    </image>
                                </ImageView>
                            </children>
                        </StackPane>
                    </children>
                </StackPane>
            </children>
            <BorderPane.margin>
                <Insets right="30.0" />
            </BorderPane.margin>
        </HBox>
    </right>
    <left>
        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <right>
                <Text fx:id="dateText" fill="#aeaeae" strokeType="OUTSIDE" strokeWidth="0.0" text="Date of recording." BorderPane.alignment="CENTER">
                    <font>
                        <Font name="Century Gothic" size="24.0" />
                    </font>
                    <BorderPane.margin>
                        <Insets left="66.0" top="12.0" />
                    </BorderPane.margin>
                </Text>
            </right>
            <center>
                <Text fx:id="nameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" textAlignment="RIGHT" BorderPane.alignment="CENTER_LEFT">
                    <font>
                        <Font name="Century Gothic" size="45.0" />
                    </font>
                    <fill>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop color="#dd7272" />
                                <Stop color="#7991eb" offset="1.0" />
                            </stops>
                        </LinearGradient>
                    </fill>
                    <BorderPane.margin>
                        <Insets left="30.0" />
                    </BorderPane.margin>
                </Text>
            </center>
        </BorderPane>
    </left>
   <bottom>
      <VBox fx:id="attemptsBox" BorderPane.alignment="CENTER" />
   </bottom>
</fx:root>
